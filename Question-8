#include <iostream>
using namespace std;

class Person {
public:
    string name;
    int age;

    void showDetails() {
        cout << "Name: " << name << endl;
        cout << "Age: " << age << endl;
    }
};

int main() {
    Person p;
    p.name = "Himesh";
    p.age = 19;
    p.showDetails();
    return 0;
}
